<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:id="@android:id/background">
        <shape>

            <gradient
                android:angle="0"
                android:startColor="#ff0002"
                android:centerColor="#D8C940"
                android:endColor="#ffe0db"
                />
            <corners android:radius="35dip" />
            <size android:height="49dp"
                android:width="190dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip >
            <shape>
                <gradient
                    android:angle="0"
                    android:startColor="#97ba93"
                    android:centerColor="#75ba70"
                    android:endColor="#0b6e07"
                    />
                <corners android:radius="35dp"
                    />
                <size android:height="49dp"
                    android:width="190dp"/>
            </shape>
        </clip>
    </item>
</layer-list>